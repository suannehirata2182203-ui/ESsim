delete this.groupFields[i],this):(console.error("Group not found. Check the group selector."),this)}addRequiredGroup(e,i,t,s){if("string"!=typeof e&&!u(e))throw Error("Group selector is not valid. Please specify a string selector or a valid DOM element.");let l;if(l="string"==typeof e?this.form.querySelector(e):e,!l)throw Error("Group selector not found! Please check the group selector.");const r=l.querySelectorAll("input"),a=Array.from(r).filter((e=>{const i=((e,i)=>{const t=[...i].reverse();for(let s=0,l=t.length;s<l;++s){const i=t[s];for(const t in e){const s=e[t];if(s.groupElem===i)return[t,s]}}return null})(this.groupFields,(e=>{let i=e;const t=[];for(;i;)t.unshift(i),i=i.parentNode;return t})(e));return!i||i[1].elems.find((i=>i!==e))})),n=this.setKeyByFieldSelector(e);return this.groupFields[n]={rules:[{rule:o.Required,errorMessage:i,successMessage:s}],groupElem:l,elems:a,touched:!1,isValid:void 0,config:t},r.forEach((e=>{this.setListeners(e)})),this}getListenerType(e){switch(e){case"checkbox":case"select-one":case"file":case"radio":return"change";default:return"input"}}setListeners(e){const i=this.getListenerType(e.type);this.removeListener(i,e,this.handlerChange),this.addListener(i,e,this.handlerChange)}clearFieldLabel(e){var i,t;null==(i=this.errorLabels[e])||i.remove(),null==(t=this.successLabels[e])||t.remove()}clearFieldStyle(e){var i,t,s,l;const r=this.fields[e],o=(null==(i=r.config)?void 0:i.errorFieldStyle)||this.globalConfig.errorFieldStyle;Object.keys(o).forEach((e=>{r.elem.style[e]=""}));const a=(null==(t=r.config)?void 0:t.successFieldStyle)||this.globalConfig.successFieldStyle||{};Object.keys(a).forEach((e=>{r.elem.style[e]=""})),r.elem.classList.remove(...c((null==(s=r.config)?void 0:s.errorFieldCssClass)||this.globalConfig.errorFieldCssClass),...c((null==(l=r.config)?void 0:l.successFieldCssClass)||this.globalConfig.successFieldCssClass))}clearErrors(){var e,i;Object.keys(this.errorLabels).forEach((e=>this.errorLabels[e].remove())),Object.keys(this.successLabels).forEach((e=>this.successLabels[e].remove()));for(const t in this.fields)this.clearFieldStyle(t);for(const t in this.groupFields){const s=this.groupFields[t],l=(null==(e=s.config)?void 0:e.errorFieldStyle)||this.globalConfig.errorFieldStyle;Object.keys(l).forEach((e=>{s.elems.forEach((i=>{var t;i.style[e]="",i.classList.remove(...c((null==(t=s.config)?void 0:t.errorFieldCssClass)||this.globalConfig.errorFieldCssClass))}))}));const r=(null==(i=s.config)?void 0:i.successFieldStyle)||this.globalConfig.successFieldStyle||{};Object.keys(r).forEach((e=>{s.elems.forEach((i=>{var t;i.style[e]="",i.classList.remove(...c((null==(t=s.config)?void 0:t.successFieldCssClass)||this.globalConfig.successFieldCssClass))}))}))}this.tooltips=[]}isTooltip(){return!!this.globalConfig.tooltip}lockForm(){const e=this.form.querySelectorAll("input, textarea, button, select");for(let i=0,t=e.length;i<t;++i)e[i].setAttribute("data-just-validate-fallback-disabled",e[i].disabled?"true":"false"),e[i].setAttribute("disabled","disabled"),e[i].style.pointerEvents="none",e[i].style.webkitFilter="grayscale(100%)",e[i].style.filter="grayscale(100%)"}unlockForm(){const e=this.form.querySelectorAll("input, textarea, button, select");for(let i=0,t=e.length;i<t;++i)"true"!==e[i].getAttribute("data-just-validate-fallback-disabled")&&e[i].removeAttribute("disabled"),e[i].style.pointerEvents="",e[i].style.webkitFilter="",e[i].style.filter=""}renderTooltip(e,i,t){var s;const{top:l,left:r,width:o,height:a}=e.getBoundingClientRect(),n=i.getBoundingClientRect(),d=t||(null==(s=this.globalConfig.tooltip)?void 0:s.position);switch(d){case"left":i.style.top=l+a/2-n.height/2+"px",i.style.left=r-n.width-5+"px";break;case"top":i.style.top=l-n.height-5+"px",i.style.left=r+o/2-n.width/2+"px";break;case"right":i.style.top=l+a/2-n.height/2+"px",i.style.left=`${r+o+5}px`;break;case"bottom":i.style.top=`${l+a+5}px`,i.style.left=r+o/2-n.width/2+"px"}i.dataset.direction=d;return{refresh:()=>{this.renderTooltip(e,i,t)}}}createErrorLabelElem(e,i,t){const s=document.createElement("div");s.innerHTML=i;const l=this.isTooltip()?null==t?void 0:t.errorLabelStyle:(null==t?void 0:t.errorLabelStyle)||this.globalConfig.errorLabelStyle;return Object.assign(s.style,l),s.classList.add(...c((null==t?void 0:t.errorLabelCssClass)||this.globalConfig.errorLabelCssClass),"just-validate-error-label"),this.isTooltip()&&(s.dataset.tooltip="true"),this.globalConfig.testingMode&&(s.dataset.testId=`error-label-${e}`),this.errorLabels[e]=s,s}createSuccessLabelElem(e,i,t){if(void 0===i)return null;const s=document.createElement("div");s.innerHTML=i;const l=(null==t?void 0:t.successLabelStyle)||this.globalConfig.successLabelStyle;return Object.assign(s.style,l),s.classList.add(...c((null==t?void 0:t.successLabelCssClass)||this.globalConfig.successLabelCssClass),"just-validate-success-label"),this.globalConfig.testingMode&&(s.dataset.testId=`success-label-${e}`),this.successLabels[e]=s,s}renderErrorsContainer(e,i){const t=i||this.globalConfig.errorsContainer;if("string"==typeof t){const i=this.form.querySelector(t);if(i)return i.appendChild(e),!0;console.error(`Error container with ${t} selector not found. Errors will be rendered as usual`)}return t instanceof Element?(t.appendChild(e),!0):(void 0!==t&&console.error("Error container not found. It should be a string or existing Element. Errors will be rendered as usual"),!1)}renderGroupLabel(e,i,t,s){if(!s){if(this.renderErrorsContainer(i,t))return}e.appendChild(i)}renderFieldLabel(e,i,t,s){var l,r,o,a,n,d,c;if(!s){if(this.renderErrorsContainer(i,t))return}if("checkbox"===e.type||"radio"===e.type){const t=document.querySelector(`label[for="${e.getAttribute("id")}"]`);"label"===(null==(r=null==(l=e.parentElement)?void 0:l.tagName)?void 0:r.toLowerCase())?null==(a=null==(o=e.parentElement)?void 0:o.parentElement)||a.appendChild(i):t?null==(n=t.parentElement)||n.appendChild(i):null==(d=e.parentElement)||d.appendChild(i)}else null==(c=e.parentElement)||c.appendChild(i)}showLabels(e,i){Object.keys(e).forEach(((t,s)=>{const l=e[t],r=this.getKeyByFieldSelector(t);if(!r||!this.fields[r])return void console.error("Field not found. Check the field selector.");const o=this.fields[r];o.isValid=!i,this.clearFieldStyle(r),this.clearFieldLabel(r),this.renderFieldError(r,!1,l),0===s&&this.globalConfig.focusInvalidField&&setTimeout((()=>o.elem.focus()),0)}))}showErrors(e){if("object"!=typeof e)throw Error("[showErrors]: Errors should be an object with key: value format");this.showLabels(e,!0)}showSuccessLabels(e){if("object"!=typeof e)throw Error("[showSuccessLabels]: Labels should be an object with key: value format");this.showLabels(e,!1)}renderFieldError(e,i=!1,t){var s,l,r,o,a,n;const d=this.fields[e];if(!1===d.isValid&&(this.isValid=!1),void 0===d.isValid||!i&&!this.isSubmitted&&!d.touched&&void 0===t)return;if(d.isValid){if(!d.asyncCheckPending){const i=this.createSuccessLabelElem(e,void 0!==t?t:d.successMessage,d.config);i&&this.renderFieldLabel(d.elem,i,null==(s=d.config)?void 0:s.errorsContainer,!0),d.elem.classList.add(...c((null==(l=d.config)?void 0:l.successFieldCssClass)||this.globalConfig.successFieldCssClass))}return}d.elem.classList.add(...c((null==(r=d.config)?void 0:r.errorFieldCssClass)||this.globalConfig.errorFieldCssClass));const u=this.createErrorLabelElem(e,void 0!==t?t:d.errorMessage,d.config);this.renderFieldLabel(d.elem,u,null==(o=d.config)?void 0:o.errorsContainer),this.isTooltip()&&this.tooltips.push(this.renderTooltip(d.elem,u,null==(n=null==(a=d.config)?void 0:a.tooltip)?void 0:n.position))}renderGroupError(e,i=!0){var t,s,l,r;const o=this.groupFields[e];if(!1===o.isValid&&(this.isValid=!1),void 0===o.isValid||!i&&!this.isSubmitted&&!o.touched)return;if(o.isValid){o.elems.forEach((e=>{var i,t;Object.assign(e.style,(null==(i=o.config)?void 0:i.successFieldStyle)||this.globalConfig.successFieldStyle),e.classList.add(...c((null==(t=o.config)?void 0:t.successFieldCssClass)||this.globalConfig.successFieldCssClass))}));const i=this.createSuccessLabelElem(e,o.successMessage,o.config);return void(i&&this.renderGroupLabel(o.groupElem,i,null==(t=o.config)?void 0:t.errorsContainer,!0))}this.isValid=!1,o.elems.forEach((e=>{var i,t;Object.assign(e.style,(null==(i=o.config)?void 0:i.errorFieldStyle)||this.globalConfig.errorFieldStyle),e.classList.add(...c((null==(t=o.config)?void 0:t.errorFieldCssClass)||this.globalConfig.errorFieldCssClass))}));const a=this.createErrorLabelElem(e,o.errorMessage,o.config);this.renderGroupLabel(o.groupElem,a,null==(s=o.config)?void 0:s.errorsContainer),this.isTooltip()&&this.tooltips.push(this.renderTooltip(o.groupElem,a,null==(r=null==(l=o.config)?void 0:l.tooltip)?void 0:r.position))}renderErrors(e=!1){if(this.isSubmitted||e||this.globalConfig.validateBeforeSubmitting){this.clearErrors(),this.isValid=!0;for(const e in this.groupFields)this.renderGroupError(e);for(const e in this.fields)this.renderFieldError(e)}}destroy(){this.eventListeners.forEach((e=>{this.removeListener(e.type,e.elem,e.func)})),Object.keys(this.customStyleTags).forEach((e=>{this.customStyleTags[e].remove()})),this.clearErrors(),this.globalConfig.lockForm&&this.unlockForm()}refresh(){this.destroy(),this.form?(this.initialize(this.form,this.globalConfig),Object.keys(this.fields).forEach((e=>{const i=this.getFieldSelectorByKey(e);i&&this.addField(i,[...this.fields[e].rules],this.fields[e].config)}))):console.error("Cannot initialize the library! Form is not defined")}setCurrentLocale(e){"string"==typeof e||void 0===e?(this.currentLocale=e,this.isSubmitted&&this.validate()):console.error("Current locale should be a string")}onSuccess(e){return this.onSuccessCallback=e,this}onFail(e){return this.onFailCallback=e,this}onValidate(e){return this.onValidateCallback=e,this}}}));

$(document).ready(function() {

    var chatWindow;

    document.addEventListener('click', function(e){
        if(e.target.classList.contains('chat-icon-open') || (e.target.parentNode && e.target.parentNode.classList.contains('chat-icon-open'))){
            e.preventDefault();
            mdt.openChatExtern();
            //initChat();
        }
    }, false);

    function initChat(){
        /*
        FOR WEB e75d2315-fa9d-4ebb-8332-4a6776436085
        */
        var campaignId = "e75d2315-fa9d-4ebb-8332-4a6776436085";
        var url =
            "https://chat-mediatel.digimobil.es/WFClient?WidgetType=WFClientChatWidget&CampaignID="+ campaignId;

        if(!chatWindow || (chatWindow && chatWindow.closed)){
            var leftMargin = window.outerWidth - 400;
            var height = window.outerHeight/2;

            chatWindow = window.open(url, "ModalPopUp",
                "toolbar=no," +
                "scrollbars=no," +
                "location=no," +
                "statusbar=no," +
                "menubar=no," +
                "resizable=0," +
                "width=400," +
                "height=" + height + ',' +
                "left=" + leftMargin + "," +
                "top=300");
        }
    }
});

window.DEV_LOGS = window.location.host.includes('entorno') || window.location.host.includes('localhost') || window.location.host.includes('-dev');
$( document ).ready(function() {
    const DAYS_COOKIES = 180;
    $(".navbar-toggler").on("click", function(e){
        e.preventDefault();
        e.stopPropagation();
        $('body').addClass("sidenav-active");
    });
    $(".sidenav-close, .sidenav-overlay").click(function(e){
        $('body').removeClass("sidenav-active");
    });

    $(document).on("submit", 'form[name="click_to_call"]', function (e) {
        e.preventDefault();
        const formErrors = $('form[name="click_to_call"] .just-validate-error-field').length;
        if (formErrors > 0){ return false }
        let post_url = $(this).attr("action");
        let request_method = $(this).attr("method");
        let form_data = $(this).serialize();

        let queueInput = $(this).find('[name="click_to_call[idQueue]"]');
        let queue = (queueInput.attr('type') === "radio")? queueInput.filter(":checked").val() : queueInput.val();

        console.log(queueInput, queue);

        $.ajax({
            url : post_url,
            type: request_method,
            data : form_data,
            "async": true,
            "dataType": "json"
        }).done(function(response){
            if (typeof dataLayer !== 'undefined') {
                dataLayer.push({
                    'event' : 'GAEvent',
                    'eventCategory' : 'c2c',
                    'eventAction' : (response.code === 200)? 'c2c_modal_resp_ok' : 'c2c_modal_resp_ko',
                    'eventLabel' : queue,
                    'eventValue' : undefined
                });
            }
            if($("#modalC2C .c2c-body").length){
                $('#modalC2C .c2c-body').html(response.msj);
            }else{
                $("#modalC2Creduced .modal-title, #modalC2C .modal-title").remove();
                $("#modalC2Creduced .modal-body, #modalC2C .modal-body").html(response.msj);
            }
        });
    });

    $(document).on("submit", 'form[name="c2c_ajax"]', function (e) {
        e.preventDefault();
        const formErrors = $('form[name="c2c_ajax"] .just-validate-error-field').length;
        if (formErrors > 0){ return false }
        let post_url = $(this).attr("action");
        let request_method = $(this).attr("method");
        let form_data = $(this).serialize();
        let body = $(this).parents('.card-body');

        let queueInput = $(this).find('[name="click_to_call[idQueue]"]');
        let queue = (queueInput.attr('type') === "radio")? queueInput.filter(":checked").val() : queueInput.val();

        $.ajax({
            url : post_url,
            type: request_method,
            data : form_data,
            "async": true,
            "dataType": "json"
        }).done(function(response){
            if (typeof dataLayer !== 'undefined') {
                dataLayer.push({
                    'event' : 'GAEvent',
                    'eventCategory' : 'c2c',
                    'eventAction' : (response.code === 200)? 'c2c_modal_resp_ok' : 'c2c_modal_resp_ko',
                    'eventLabel' : queue,
                    'eventValue' : undefined
                });
            }

            body.html(response.msj);
        });
    });

    $(document).on("submit", 'form[name="c2c_extended_ajax"]', function (e) {
        e.preventDefault();
        const formErrors = $('form[name="c2c_extended_ajax"] .just-validate-error-field').length;
        if (formErrors > 0){ return false }
        let post_url = $(this).attr("action");
        let request_method = $(this).attr("method");
        let form_data = $(this).serialize();
        let body = $(this).parents('.modal-content');

        let queueInput = $(this).find('[name="click_to_call[idQueue]"]');
        let queue = (queueInput.attr('type') === "radio")? queueInput.filter(":checked").val() : queueInput.val();

        $.ajax({
            url : post_url,
            type: request_method,
            data : form_data,
            "async": true,
            "dataType": "json"
        }).done(function(response){
            if (typeof dataLayer !== 'undefined') {
                dataLayer.push({
                    'event' : 'GAEvent',
                    'eventCategory' : 'c2c',
                    'eventAction' : (response.code === 200)? 'c2c_modal_resp_ok' : 'c2c_modal_resp_ko',
                    'eventLabel' : queue,
                    'eventValue' : undefined
                });
            }

            body.find(".modal-body").html(response.msj);
        });
    });

    $(document).on("submit", 'form[name="c2c_extended_desktop"]', function (e) {
        e.preventDefault();

        const formErrors = $('form[name="c2c_extended_desktop"] .just-validate-error-field').length;
        if (formErrors > 0){ return false }

        let post_url = $(this).attr("action");
        let request_method = $(this).attr("method");
        let form_data = $(this).serialize();
        let $modal = $("#modalC2Cextended");

        let queueInput = $(this).find('[name="click_to_call[idQueue]"]');
        let queue = (queueInput.attr('type') === "radio")? queueInput.filter(":checked").val() : queueInput.val();

        $modal.modal('show');
        $.ajax({
            url : post_url,
            type: request_method,
            data : form_data,
            "async": true,
            "dataType": "json"
        }).done(function(response){
            if (typeof dataLayer !== 'undefined') {
                dataLayer.push({
                    'event' : 'GAEvent',
                    'eventCategory' : 'c2c',
                    'eventAction' : (response.code === 200)? 'c2c_modal_resp_ok' : 'c2c_modal_resp_ko',
                    'eventLabel' : queue,
                    'eventValue' : undefined
                });
            }

            $modal.find(".modal-body").html(response.msj);
        });
    });

    $('#modalC2C').on('show.bs.modal', function (event) {
        $('#modalC2C .c2c-body').html("");
        if (window['validate-click_to_call']) {
            window['validate-click_to_call'].destroy();
        }
        global.ajax(
            "/api/clickToCall",
            "GET",
            false,
            false,
            true,
            function (r) {
                $('#modalC2C .c2c-body').html(r);
                dataLayer.push({'event': 'GAEvent','eventAction': 'c2c_modal_open',});
                validateForm('click_to_call')
            },
            'html'
        );
    })

    $('#modalC2Creduced').on('show.bs.modal', function (event) {
        let idQueue = $(this).data("queue");
        $('#modalC2Creduced .modal-body').html("");
        global.ajax(
            "/api/clickToCall/" + idQueue,
            "GET",
            false,
            false,
            true,
            function (r) {
                $('#modalC2Creduced .modal-body').html(r);
                validateForm('click_to_call')
            },
            'html'
        );
    })

    $("[data-action=openC2C]").on("click", function (e){
        e.preventDefault();
        let $modal = $($(this).data("target"));
        let idQueue = $modal.data("queue");

        $modal.find('.modal-body').html("");
        $modal.modal("show");
        global.ajax(
            "/api/clickToCall/extended/" + idQueue,
            "GET",
            false,
            false,
            true,
            function (r) {
                $modal.find('.modal-body').html(r);
                validateForm('click_to_call')
            },
            'html'
        );
    });

    $(document).on('click', '#modal_c2c_chat', function(){
        dataLayer.push({'event': 'GAEvent','eventAction': 'c2c_modal_link_chat',});
    })
    $(document).on('click', '#modal_c2c_tel', function(){
        dataLayer.push({'event': 'GAEvent','eventAction': 'c2c_modal_link_phone',});
    })

    $('form[name="check_number"]').submit(function(e){
        e.preventDefault();
        const formErrors = $('form[name="check_number"] .just-validate-error-field').length;
        if (formErrors > 0){ return false }

        let post_url = $(this).attr("action");
        let request_method = $(this).attr("method");
        let form_data = $(this).serialize();
        let $this = $(this);
        let buttonSubmit = $(this).find("button[type=submit]");

        if ($(this).find("#check_number_phone_first").val() === $(this).find("#check_number_phone_second").val()){

            buttonSubmit.attr('disabled', 'disabled').html('<span class="spinner-border spinner-border-sm" role="status" aria-hidden="true"></span> ' + buttonSubmit.data("text-disabled"));

            $.ajax({
                url : post_url,
                type: request_method,
                data : form_data,
                "async": true,
                "dataType": "json"
            }).done(function(response){
                if (response.code === 200){
                    let rechargeForm = $('form[name="recharge_number"]');
                    $('form[name="check_number"]').hide();

                    rechargeForm.find("#recharge_number_id").val(response.data.id);
                    rechargeForm.find(".phone span").text(response.data.number);
                    rechargeForm.find("#recharge_number_phone").val(response.data.number);
                    rechargeForm.show();
                }else{
                    $this.find("button[type=submit]").attr('disabled', false);
                    $(".form-response-alert .alert-danger").html(response.msj);

                    buttonSubmit.attr('disabled', false).html(buttonSubmit.data("text-active"));
                }
            });
        }else{
            $(".form-response-alert .alert-danger").html("El nmero no coincide");
        }
    });

    $('form[name="check_number"], form[name="check_invoice"]').on("change", "input", function (e) {
        $(this).parents("form").find('.form-response-alert .alert-danger').html("");
    });

    $('form[name="recharge_number"] input[data-type], form[name="recharge_number"] input[type=radio]').change(function(e){
        e.preventDefault();

        let error = $('form[name="recharge_number"] input[data-type]').parents(".form-group").find(".form-error");
        if ($(this).attr('type') === "radio"){
            $('form[name="recharge_number"] input[data-type]').val($(this).val());
            $('form[name="recharge_number"] .amount span').text($(this).val() + "");

            error.html('');
            $('form[name="recharge_number"] button').attr('disabled', false);
        }else{
            // let amount = parseInt($(this).val());
            // if ((amount % 5 !== 0 && amount !== 7) || amount > 50){
            //     error.html('<span class="invalid-feedback d-block"><span class="d-block"><span class="form-error-icon badge badge-danger text-uppercase">Error</span> <span class="form-error-message">El importe tiene que ser mltiplo de 5, hasta un mximo de 50 euros. O si lo prefieres, puedes recargar 7 euros.</span></span></span>');
            //     //$('form[name="recharge_number"] button').attr('disabled', 'disabled');
            // }else{
            //     error.html('');
            //     $('form[name="recharge_number"] button').attr('disabled', false);
            // }
        }
    });

    $('form[name="check_invoice"]').submit(function(e){
        e.preventDefault();
        const formErrors = $('form[name="check_invoice"] .just-validate-error-field').length;
        if (formErrors > 0){ return false }

        let post_url = $(this).attr("action");
        let request_method = $(this).attr("method");
        let form_data = $(this).serialize();
        let buttonSubmit = $(this).find("button[type=submit]");

        buttonSubmit.attr('disabled', 'disabled').html('<span class="spinner-border spinner-border-sm" role="status" aria-hidden="true"></span> ' + buttonSubmit.data("text-disabled"));

        $.ajax({
            url : post_url,
            type: request_method,
            data : form_data,
            "async": true,
            "dataType": "json"
        }).done(function(response){
            if (response.code === 200){
                let paymentForm = $('form[name="payment_invoice"]');
                $('form[name="check_invoice"]').hide();

                paymentForm.find("#payment_invoice_invoice").val(response.data.invoice);
                paymentForm.find("#payment_invoice_client").val(response.data.client);
                paymentForm.find("#payment_invoice_amount").val(response.data.amount);
                paymentForm.show();
            }else{
                $(".form-response-alert .alert-danger").html(response.msj);
                buttonSubmit.attr('disabled', false).html(buttonSubmit.data("text-active"));
            }
        }).fail(function(response) {
            $(".form-response-alert .alert-danger").html(response.title);
            buttonSubmit.attr('disabled', false).html(buttonSubmit.data("text-active"));
        });
    });

    $('form[name="sponsorship_inscriptions"], form[name="sponsorship_inscriptions_doc"]').submit(function (e) {
        e.preventDefault();

        const formErrors = $('form[name="sponsorship_inscriptions"] .just-validate-error-field').length;
        if (formErrors > 0){ return false }

        let post_url = $(this).attr("action");
        let request_method = $(this).attr("method");
        let form_data = $(this).serialize();

        $.ajax({
            url: post_url,
            type: request_method,
            data: form_data,
            "async": true,
            "dataType": "json"
        }).done(function (response) {
            let str = "";

            switch (response.code) {
                case 200:
                    str = `
                        <img src="/images/ico_ok_green.svg" alt="OK" />
                        <p class="my-3" id="titleOK" style="color: #00AA70; font-size: 1.25rem; font-weight: bold;">Solicitud enviada</p>
                        <p id="textOK">Muchas gracias por participar! </p>
                        <p class="center">
                            <button type="button" class="btn btn-primary" data-dismiss="modal"> CERRAR </button>
                        </p>
                    `;

                    break;
                case 401:
                    str = `
                        <img src="/images/ico_info_orange.svg" alt="ERROR" style="width: 50px" />
                        <p class="my-3" id="titleKO" style="color:#F18700; font-size: 1.25rem; font-weight: bold;">Ya hemos registrado tus datos con anterioridad</p>
                        <p id="textKO">Muchas gracias por participar!</p>
                        <p class="center">
                            <button type="button" class="btn btn-primary" data-dismiss="modal"> CERRAR </button>
                        </p>
                    `;
                    break;
                default:
                    str = `
                        <img src="/images/ico_alert_red.svg" alt="ERROR" />
                        <p class="my-3" id="titleKO" style="color:#D60B52; font-size: 1.25rem; font-weight: bold;">Se ha producido un error</p>
                        <p id="textKO">Puedes intentarlo de nuevo ms tarde.</p>
                        <p class="center">
                            <button type="button" class="btn btn-primary back"> VOLVER </button>
                        </p>
                    `;
            }

            $("#modal").modal("show");
            $("#modal form").hide();
            $("#modal #response").html(str).show();
        });
    });

    $(".accordionQuestions .question").click(function(e){
        let answer = $(this).parents("li").find(".answer");

        if (!$(this).parents("li").hasClass("active")){
            let data = {
                topic: $(this).data("topic"),
                cat: $(this).data("cat")
            };

            global.ajax(
                "/api/faqs/view",
                "POST",
                JSON.stringify(data),
                false,
                true,
                function (r) {

                }
            );
        }

        $(this).parents("li").toggleClass("active");
        answer.slideToggle();

    });

    $(".accordionQuestions .vote [data-action]").click(function(e){
        let father = $(this).parents("li");

        let data = {
            topic: father.find(".question").data("topic"),
            action: $(this).data("action")
        };

        global.ajax(
            "/api/faqs/vote",
            "POST",
            JSON.stringify(data),
            false,
            true,
            function (r) {
                if (parseInt(r.code) === 200){
                    father.find(".vote").remove();
                    father.find(".answer").append(r.msj);
                }
            }
        );

    });

    $("ul.mostViewed li.more").click(function(e){
        $(this).toggleClass("active");
        $("ul.mostViewed+.next").slideToggle();

    });

    $(document).on("click", "form[name=faq_search] button.reset", function (e) {
        e.stopPropagation();
        $(this).parents("form").find("input[type=text]").val("");
        $(this).removeClass("reset").addClass("search").html(`<svg xmlns="http://www.w3.org/2000/svg" width="21.053" height="25.805" viewBox="0 0 21.053 25.805"><g id="Grupo_3501" data-name="Grupo 3501" transform="translate(1 1)"><g id="ico_salir_white" transform="translate(0)"><circle id="Elipse_81" data-name="Elipse 81" cx="9.164" cy="9.164" r="9.164" fill="transparent" stroke="#005181" stroke-linecap="round" stroke-linejoin="round" stroke-width="2"/><path id="Trazado_1067" data-name="Trazado 1067" d="M8.718,13.077,4.5,6.75" transform="translate(9.949 10.341)" fill="#fff" stroke="#005181" stroke-linecap="round" stroke-width="2"/></g></g></svg>`);
    });

    $(document).on("click", "form[name=faq_search] button.search", function (e) {
        let form = $(this).parents("form");
        if (form.find("input#faq_search_search").val().match(/[~@#$%*()|+=<,.>\[{\]}"']/)) {
            form.addClass("error");
            e.preventDefault();
        }else{
            form.submit();
        }
    });

    $(document).on("keyup", "form[name=faq_search] input#faq_search_search", function (e) {
        let form = $(this).parents("form");
        form.find("button").removeClass("reset").addClass("search").html(`<svg xmlns="http://www.w3.org/2000/svg" width="21.053" height="25.805" viewBox="0 0 21.053 25.805"><g id="Grupo_3501" data-name="Grupo 3501" transform="translate(1 1)"><g id="ico_salir_white" transform="translate(0)"><circle id="Elipse_81" data-name="Elipse 81" cx="9.164" cy="9.164" r="9.164" fill="transparent" stroke="#005181" stroke-linecap="round" stroke-linejoin="round" stroke-width="2"/><path id="Trazado_1067" data-name="Trazado 1067" d="M8.718,13.077,4.5,6.75" transform="translate(9.949 10.341)" fill="#fff" stroke="#005181" stroke-linecap="round" stroke-width="2"/></g></g></svg>`);
        if (!form.find("input#faq_search_search").val().match(/[~@#$%*()|+=<,.>\[{\]}"']/)) {
            form.removeClass("error");
            form.find("button").attr("disabled", false);
        }
        else{
            form.addClass("error").find("button").attr("disabled", "disabled");
            e.preventDefault();
        }
    });

    $(document).on("submit", "body:not([class^=adminPanel]) form[name=faq_search]", function (e) {
        let form = $(this);
        if (form.find("input#faq_search_search").val().match(/[~@#$%*()|+=<,.>\[{\]}"']/)) {
            form.addClass("error");
            e.preventDefault();
        }
        else{
            if (form.find("input#faq_search_search").val().length === 0){
                form.addClass("error");
                e.preventDefault();
            }
        }
    });

    $(document).on('click', '[data-url]', function () {
        let url = $(this).attr("data-url");
        let md = $(this).data("target");
        let linkButton = $(md).find("a.btn");
        if (url === "javascript:"){
            linkButton.attr("data-dismiss", "modal");
        }else{
            linkButton.attr("data-dismiss", "");
        }
        linkButton.attr("href",url);
    });

    function setCookie(cname,cvalue,exdays)
    {
        let d = new Date();
        d.setTime(d.getTime() + (exdays*24*60*60*1000));
        let expires = "expires=" + d.toGMTString();
        let domain = '';
        if (location.hostname.includes("digimobil.es")){
            domain = ";domain=digimobil.es";
        }
        document.cookie = cname + "=" + cvalue + "; " + expires + ";path=/" + domain;
    }

    $(document).on("click", '[data-gtm]', function (e) {
        if (typeof dataLayer !== 'undefined') {
            dataLayer.push({
                'event': ($(this).data("gtm-event") !== undefined && $(this).data("gtm-event").length > 0) ? $(this).data("gtm-event") :'GAEvent',
                'eventCategory': $(this).data("gtm-category"),
                'eventAction': $(this).data("gtm-action"),
                'eventLabel': $(this).data("gtm-label"),
                'eventValue': ($(this).data("gtm-value") !== undefined && $(this).data("gtm-value").length > 0)? $(this).data("gtm-value") :  undefined,
                'eventLabelExtra': ($(this).data("gtm-labelextra") !== undefined && $(this).data("gtm-labelextra").length > 0)? $(this).data("gtm-labelextra") :  undefined
            });
        }
    });

    window.addEventListener('CookiebotOnAccept', function (e) {
        if (Cookiebot.consent.marketing){
            setCookie('adsConsent',true,DAYS_COOKIES);
        }else{
            setCookie('adsConsent',false,DAYS_COOKIES);
        }

        if (Cookiebot.consent.statistics){
            setCookie('analyticsConsent',true,DAYS_COOKIES);
        }else{
            setCookie('analyticsConsent',false,DAYS_COOKIES);
        }
        global.consentAction();

        if ($("main#cookies").length > 0){
            global.updateButtonsCookiePage();
        }

    }, false);
    window.addEventListener('CookiebotOnDecline', function (e) {
        setCookie('adsConsent',false,DAYS_COOKIES);
        setCookie('analyticsConsent',false,DAYS_COOKIES);
        global.consentAction();

        if ($("main#cookies").length > 0){
            global.updateButtonsCookiePage();
        }

    }, false);

    $("body").tooltip({ selector: '[data-toggle=tooltip]' });

    $('.reviews #recipeCarousel').carousel();

    $('.reviews .carousel .carousel-item').each(function(){
        let minPerSlide = 3;
        let next = $(this).next();
        if (!next.length) {
            next = $(this).siblings(':first');
        }
        next.children(':first-child').clone().appendTo($(this));

        for (let i=0;i<minPerSlide;i++) {
            next=next.next();
            if (!next.length) {
                next = $(this).siblings(':first');
            }

            next.children(':first-child').clone().appendTo($(this));
        }
    });

    $(document).on("click", '[data-copy-clipboard]', function (e) {
        e.preventDefault();
        navigator.clipboard.writeText($(this).data('copy-clipboard'));

        $.toast({
            title: "",
            subtitle: "",
            content: `Enlace copiado`,
            type: 'success',
            delay: 5000,
            dismissible: false
        });
    });

    $(document).on("change", "form.productType input", function(e){
        e.preventDefault();

        $("#postpaid, #prepaid").hide();
        $("#" + $(this).attr("value")).show();
    });

    $(document).on("click", "#TV .categorySelector a[data-type]", function(e){
        e.preventDefault();

        let newCat = $(this).data("type");
        $("#TV .categorySelector a").removeClass("active");
        $(this).addClass("active");

        $("#TV .listChannels .channel").hide();
        $(`#TV .listChannels .channel[data-cat=${newCat}]`).show();

        // console.log(newCat);
    });

    $(document).on('click', '[data-scroll]', function(e) {
        e.preventDefault();
        let scrollTo = $(this).attr('href');
        let targetElem = $(scrollTo);

        if (targetElem.length) {
            $('html').animate({
                scrollTop: targetElem.offset().top - 105
            }, 100);
        }
    });

    function rewriteCombinationLinks(container) {
        const localTarget = window.location.origin + window.location.pathname;
        
        console.log('[REWRITE] Starting rewriteCombinationLinks, container:', container);
        
        //     
        const $allCards = $(container).find('.col, .card, [class*="combination"], [class*="tariff"], [class*="product"]').filter(function() {
            //   ,     combinacion
            return $(this).find('a[href*="combinacion"], a[href*="combina-telefonia"]').length > 0;
        });
        
        console.log('[REWRITE] Found', $allCards.length, 'cards with combinacion links');
        
        //      DOM
        $allCards.each(function(index) {
            if (index >= 6) return; //  6 
            
            const $card = $(this);
            const cardText = $card.text().toUpperCase();
            
            //       
            let targetPage = detectTarifByContent($card);
            
            if (!targetPage) return;
            
            console.log(`[REWRITE] Card #${index}  ${targetPage}`);
            
            //           targetPage
            $card.find('a').each(function() {
                const $link = $(this);
                const href = $link.attr('href') || '';
                const text = $link.text().trim().toUpperCase();
                
                //     URL
                if (href.includes('movil=1581-1581') || href.includes('movil=1581')) {
                    $link.attr('href', '/tarif4/index.html');
                    return;
                }
                if (href.includes('movil=1562') || href.includes('movil=1326')) {
                    $link.attr('href', '/tarif2/index.html');
                    return;
                }
                if (href.includes('movil=1561') || href.includes('movil=1286')) {
                    $link.attr('href', '/tarif1/index.html');
                    return;
                }
                if (href.includes('movil=1580') || href.includes('movil=1311')) {
                    $link.attr('href', '/tarif3/index.html');
                    return;
                }
                if (href.includes('movil=1582-1582-1582') || href.includes('movil=1582')) {
                    $link.attr('href', '/tarif5/index.html');
                    return;
                }
                
                //  targetPage      (  MS INFORMACIN)
                if (href.includes('combinacion') || href.includes('combina-telefonia') ||
                    text.includes('ME INTERESA') || text.includes('MS INFORMACIN') || text.includes('MAS INFORMACION') ||
                    text.includes('CONTRATAR') || text.includes('LO QUIERO')) {
                    $link.attr('href', targetPage);
                }
            });
        });
    }
    
    // ============================================
    //      COMBINACION
    // ============================================
    
    // 1.        (  !)
    function detectTarifByContent($card) {
        if (!$card || !$card.length) return null;
        
        const cardText = $card.text();
        
        // :       
        if (cardText.includes('22 /mes') || cardText.includes('22/mes') || cardText.match(/22\s*\/mes/i)) {
            return '/tarif4/index.html';
        }
        if (cardText.includes('13 /mes') || cardText.includes('13/mes') || cardText.match(/13\s*\/mes/i)) {
            return '/tarif1/index.html';
        }
        if (cardText.includes('15 /mes') || cardText.includes('15/mes') || cardText.match(/15\s*\/mes/i)) {
            return '/tarif2/index.html';
        }
        if (cardText.includes('18 /mes') || cardText.includes('18/mes') || cardText.match(/18\s*\/mes/i)) {
            return '/tarif3/index.html';
        }
        if (cardText.includes('25 /mes') || cardText.includes('25/mes') || cardText.match(/25\s*\/mes/i)) {
            return '/tarif5/index.html';
        }
        
        // Fallback:    ,  "CREA TU PROPIA COMBINACIN"
        const cardTextUpper = cardText.toUpperCase();
        if (cardTextUpper.includes('CREA TU PROPIA') || cardTextUpper.includes('COMBINACIN')) {
            return '/tarif6/index.html';
        }
        
        return null;
    }
    
    // 2.     combinacion
    function rewriteCombinacionLink(linkElement) {
        const $link = $(linkElement);
        const href = $link.attr('href') || '';
        const text = $link.text().trim().toUpperCase();
        
        if (!href || (!href.includes('combinacion') && !href.includes('combina-telefonia'))) return;
        
        //    tarif4  URL (x2)
        if (href.includes('movil=1581-1581') || href.includes('movil=1581') || href.includes('ilimitodo-x2')) {
            $link.attr('href', '/tarif4/index.html');
            return;
        }
        //    tarif5  URL (x3)
        if (href.includes('movil=1582-1582-1582') || href.includes('movil=1582') || href.includes('ilimitodo-x3')) {
            $link.attr('href', '/tarif5/index.html');
            return;
        }
        //    tarif3  URL ( ilimitodo,  x2/x3)
        if (href.includes('movil=1580') || href.includes('movil=1311') || (href.includes('ilimitodo') && !href.includes('ilimitodo-x2') && !href.includes('ilimitodo-x3'))) {
            $link.attr('href', '/tarif3/index.html');
            return;
        }
        
        //   
        const $card = $link.closest('.col, .card, [class*="combination"], [class*="tariff"], [class*="product"]');
        
        //       
        let targetPage = detectTarifByContent($card.length ? $card : $link.parent().parent().parent());
        
        //     -  href
        if (targetPage) {
            $link.attr('href', targetPage);
            console.log(`[REWRITE LINK] "${text}" (${href})  ${targetPage}`);
        }
    }
    
    // 2. MutationObserver -    
    const observer = new MutationObserver(function(mutations) {
        mutations.forEach(function(mutation) {
            mutation.addedNodes.forEach(function(node) {
                if (node.nodeType === 1) { // Element node
                    const $node = $(node);
                    
                    //     combinacion   
                    $node.find('a[href*="combinacion"], a[href*="combina-telefonia"]').each(function() {
                        rewriteCombinacionLink(this);
                    });
                    
                    //       
                    if ($node.is('a[href*="combinacion"], a[href*="combina-telefonia"]')) {
                        rewriteCombinacionLink(node);
                    }
                    
                    //    -    
                    if ($node.is('.col, .card, [class*="combination"], [class*="tariff"], [class*="product"]')) {
                        $node.find('a[href*="combinacion"], a[href*="combina-telefonia"]').each(function() {
                            rewriteCombinacionLink(this);
                        });
                    }
                }
            });
        });
    });
    
    //      DOM
    if (document.body) {
        observer.observe(document.body, {
            childList: true,
            subtree: true
        });
    } else {
        $(document).ready(function() {
            observer.observe(document.body, {
                childList: true,
                subtree: true
            });
        });
    }
    
    // 3.    -        ME INTERESA
    $(document).on('click', 'a', function(e) {
        const $link = $(this);
        const href = $link.attr('href') || '';
        const text = $link.text().trim().toUpperCase();
        
        //     URL
        if (href.includes('movil=1581-1581') || href.includes('movil=1581') || href.includes('ilimitodo-x2')) {
            e.preventDefault();
            window.location.href = '/tarif4/index.html';
            return false;
        }
        if (href.includes('movil=1562') || href.includes('movil=1326')) {
            e.preventDefault();
            window.location.href = '/tarif2/index.html';
            return false;
        }
        if (href.includes('movil=1561') || href.includes('movil=1286')) {
            e.preventDefault();
            window.location.href = '/tarif1/index.html';
            return false;
        }
        if (href.includes('movil=1580') || href.includes('movil=1311') || (href.includes('ilimitodo') && !href.includes('ilimitodo-x2') && !href.includes('ilimitodo-x3'))) {
            e.preventDefault();
            window.location.href = '/tarif3/index.html';
            return false;
        }
        if (href.includes('movil=1582-1582-1582') || href.includes('movil=1582') || href.includes('ilimitodo-x3')) {
            e.preventDefault();
            window.location.href = '/tarif5/index.html';
            return false;
        }
        
        //   
        const $card = $link.closest('.col, .card, [class*="combination"], [class*="tariff"], [class*="product"]');
        
        //    -    
        if ($card.length) {
            let targetPage = detectTarifByContent($card);
            
            //        ME INTERESA  MS INFORMACIN - 
            if (targetPage && (text.includes('ME INTERESA') || text.includes('MS INFORMACIN') || text.includes('MAS INFORMACION') || 
                text.includes('CONTRATAR') || text.includes('LO QUIERO'))) {
                e.preventDefault();
                e.stopPropagation();
                e.stopImmediatePropagation();
                window.location.href = targetPage;
                return false;
            }
        }
    });
    
    // 4.     (   - )
    setInterval(function() {
        $('a[href*="combinacion"], a[href*="combina-telefonia"]').each(function() {
            rewriteCombinacionLink(this);
        });
    }, 2000); //   2 

    //      
    function loadCombinations() {
        const $lazyElements = $('.combinations [data-lazy-load]');
        console.log('[COMBINATIONS] Found elements:', $lazyElements.length);
        
        if ($lazyElements.length === 0) {
            console.warn('[COMBINATIONS] No elements found, retrying in 500ms...');
            setTimeout(loadCombinations, 500);
            return;
        }
        
        $lazyElements.each(function(index){
            const $container = $(this);
            const section = $container.attr("data-lazy-load");
            const zone = $container.attr("data-lazy-zone");
            
            console.log('[COMBINATIONS] Loading:', {index, section, zone, element: $container[0]});
            
            if (!section || !zone) {
                console.error('[COMBINATIONS] Missing attributes:', {section, zone});
                return;
            }
            
            //  loader
            $container.html(`<div class="col loader"><span></span></div>`);
            
            //      API (    Railway)
            tryDirectApiRequest($container, zone, section);
        });
    }
    
    //      API (fallback)
    function tryDirectApiRequest($container, zone, section) {
        const originalApiUrl = "https://www.digimobil.es/api/v1/combinations/" + zone + "/" + section;
        console.log('[COMBINATIONS] Trying direct API request as final fallback...');
        $.ajax({
            url: originalApiUrl,
            method: "GET",
            dataType: "html",
            timeout: 10000,
            crossDomain: true,
            success: function (r) {
                if (r && r.trim().length > 0) {
                    $container.html(r);
                    rewriteCombinationLinks($container);
                    console.log('[COMBINATIONS] Direct API request succeeded');
                } else {
                    //   -     
                    $container.html('');
                }
            },
            error: function() {
                //   -     ,   
                $container.html('');
                console.log('[COMBINATIONS] All fallbacks failed, hiding container silently');
            }
        });
    }
    
    //     
    if (document.readyState === 'loading') {
        $(document).ready(loadCombinations);
    } else {
        // DOM  
        loadCombinations();
    }
});

$.cachedScript = function( url, options ) {
    options = $.extend( options || {}, {
        dataType: "script",
        cache: true,
        url: url
    });
    return jQuery.ajax( options );
};

var global = function () {
    function addEventListeners() {
        if (global.getCookie("CookieConsent") === null){
            global.consentAction();
        }
        if ($("main#cookies").length > 0){
            global.updateButtonsCookiePage();
        }
    }

    function loadLocale() {
        global.ajax(
            "/assets/" + $("html").data("assets") + "/locales/javascript." + $("html").attr("lang") + ".json",
            "GET",
            [],
            false,
            true,
            function (r) {
                window.i18n = r;
            }
        );
    }

    return {
        init: function(){
            $.ajaxSetup({
                cache: true
            });
            addEventListeners();
            loadLocale();
        },
        ajax: function (url, type, data, sendAuth = false, async = true, callback = function(){}, dataType = "json") {
            var beforeSend = function (xhr) { };
            var headers = {};

            if (type === "POST") {
                headers = {"content-type": "application/json"};
            }

            return $.ajax({
                "method": type,
                "url": url,
                "data": data,
                "dataType": dataType,
                "crossDomain": true,
                "async": async,
                beforeSend: beforeSend,
                headers: headers,
                fail: function (xhr, ajaxOptions, thrownError) {
                    console.log(xhr);
                    console.log(ajaxOptions);
                    console.log(thrownError);
                },
                statusCode: {
                    401: function (xhr) {
                        global.logout();
                    }
                }
            })
                .done(function (data, textStatus, promise) {
                    callback(data);
                    return promise;
                })
                .fail(function (xhr, ajaxOptions, thrownError) {
                    if(xhr.status !== 401){
                        $.toast({
                            title: "",
                            subtitle: "",
                            content: thrownError,
                            type: 'error',
                            delay: 5000,
                            dismissible: true
                        });
                    }
                })
                .always(function () {

                })
                .then(function (data, textStatus, promise) {
                    return promise;
                });
        },
        formatNumber: function(number, decimals = 2) {
            var n = !isFinite(+number) ? 0 : +number,
                prec = !isFinite(+decimals) ? 0 : Math.abs(decimals),
                sep = (typeof thousands_sep === 'undefined') ? '.' : thousands_sep,
                dec = (typeof dec_point === 'undefined') ? ',' : dec_point,
                toFixedFix = function (n, prec) {
                    var k = Math.pow(10, prec);
                    return Math.round(n * k) / k;
                },
                s = (prec ? toFixedFix(n, prec) : Math.round(n)).toString().split('.');
            if (s[0].length > 3) {
                s[0] = s[0].replace(/\B(?=(?:\d{3})+(?!\d))/g, sep);
            }
            if ((s[1] || '').length < prec) {
                s[1] = s[1] || '';
                s[1] += new Array(prec - s[1].length + 1).join('0');
            }
            return s.join(dec);
        },
        getCookie: function (cname) {
            let name = cname + "=";
            let ca = document.cookie.split(';');
            for(let i = 0; i < ca.length; i++) {
                let c = ca[i];
                while (c.charAt(0) === ' ') {
                    c = c.substring(1);
                }
                if (c.indexOf(name) === 0) {
                    return c.substring(name.length, c.length);
                }
            }
            return null;
        },
        validateDNIe: function (documentNumber) {
            let validChars = 'TRWAGMYFPDXBNJZSQVHLCKET';
            let nifRexp = /^[0-9]{8}[TRWAGMYFPDXBNJZSQVHLCKET]{1}$/i;
            let nieRexp = /^[XYZ]{1}[0-9]{7}[TRWAGMYFPDXBNJZSQVHLCKET]{1}$/i;
            let str = documentNumber.toString().toUpperCase();
            if (!nifRexp.test(str) && !nieRexp.test(str)) return false;
            let nie = str
                .replace(/^[X]/, '0')
                .replace(/^[Y]/, '1')
                .replace(/^[Z]/, '2');
            let letter = str.substr(-1);
            let charIndex = parseInt(nie.substr(0, 8)) % 23;
            if (validChars.charAt(charIndex) === letter) return true;

            return false;
        },
        validateDNI: function (doc) {
            let dni_letters = "TRWAGMYFPDXBNJZSQVHLCKE";
            let letter = dni_letters.charAt( parseInt( doc, 10 ) % 23 );

            return letter === doc.charAt(8);
        },
        validateNIE: function (doc) {
            let nie_prefix = doc.charAt(0);

            switch (nie_prefix) {
                case 'X': nie_prefix = 0; break;
                case 'Y': nie_prefix = 1; break;
                case 'Z': nie_prefix = 2; break;
                default: return false;
            }
            return global.validateDNI( nie_prefix + doc.substr(1) );
        },
        validateCIF: function (documentNumber) {
            if (!documentNumber || documentNumber.length !== 9) {
                return false;
            }

            var letters = ['J', 'A', 'B', 'C', 'D', 'E', 'F', 'G', 'H', 'I'];
            var digits = documentNumber.substr(1, documentNumber.length - 2);
            var letter = documentNumber.substr(0, 1);
            var control = documentNumber.substr(documentNumber.length - 1);
            var sum = 0;
            var i;
            var digit;

            if (!letter.match(/[A-Z]/)) {
                return false;
            }

            for (i = 0; i < digits.length; ++i) {
                digit = parseInt(digits[i]);

                if (isNaN(digit)) {
                    return false;
                }

                if (i % 2 === 0) {
                    digit *= 2;
                    if (digit > 9) {
                        digit = parseInt(digit / 10) + (digit % 10);
                    }

                    sum += digit;
                } else {
                    sum += digit;
                }
            }

            sum %= 10;
            if (sum !== 0) {
                digit = 10 - sum;
            } else {
                digit = sum;
            }

            if (letter.match(/[ABEH]/)) {
                return String(digit) === control;
            }
            if (letter.match(/[NPQRSW]/)) {
                return letters[digit] === control;
            }

            return String(digit) === control || letters[digit] === control;
        },
        consentAction: function () {
            $('[data-consent-action]').each(function() {
                let action = $(this).data("consent-action");
                let consentRequired = $(this).data("consent-required");
                let consentStatus = (typeof Cookiebot !== 'undefined' )? Cookiebot.consent[consentRequired] : false;

                switch (action){
                    case 'youtube':
                        let youtubeID = $(this).data("youtube-id");

                        if (consentStatus){
                            $(this).html('<div class="embed-responsive embed-responsive-16by9"><iframe class="embed-responsive-item" src="https://www.youtube-nocookie.com/embed/' + youtubeID + '" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen=""></iframe></div>');
                        }else{
                            $(this).html('<div class="claim">Para ver este contenido, necesitamos que aceptes nuestras cookies. Siempre puedes cambiar tus preferencias desde <a href="javascript:Cookiebot.renew();">aqu</a></div><img src="https://i3.ytimg.com/vi/' + youtubeID + '/maxresdefault.jpg" class="img-fluid">');
                        }
                        break;

                    case 'status':
                        if(consentStatus){
                            if (consentRequired === 'necessary'){
                                $(this).find('span').html(`<svg xmlns="http://www.w3.org/2000/svg" width="14" height="16" viewBox="0 0 14 16" fill="none"><path d="M12.5393 15.1136C12.7426 15.1136 12.9085 14.9493 12.9085 14.7466V6.85384C12.9085 6.65101 12.7426 6.48638 12.5393 6.48638H1.46144C1.25642 6.48638 1.09144 6.65101 1.09144 6.85384V14.7466C1.09144 14.9493 1.25642 15.1136 1.46144 15.1136H12.5393ZM7.09289 8.50586C7.75367 8.50586 8.29344 9.0599 8.29344 9.74108C8.29344 10.216 8.02399 10.6543 7.60567 10.8577V12.4298H6.5788V10.8577C6.16179 10.6543 5.89321 10.216 5.89321 9.74108C5.89321 9.0599 6.43211 8.50586 7.09289 8.50586ZM12.5393 16H1.46144C0.765838 16 0.199951 15.4377 0.199951 14.7466V6.85384C0.199951 6.16248 0.765838 5.6 1.46144 5.6H12.5393C13.2345 5.6 13.8 6.16248 13.8 6.85384V14.7466C13.8 15.4377 13.2345 16 12.5393 16ZM7.09289 9.39224C6.92573 9.39224 6.7847 9.552 6.7847 9.74108C6.7847 9.8785 6.85979 10.0075 6.97144 10.062L7.09202 10.1208L7.2139 10.0615C7.32642 10.0068 7.40195 9.87807 7.40195 9.74108C7.40195 9.552 7.26048 9.39224 7.09289 9.39224Z" fill="#ECF8FC"/><path d="M7.70282 12.266H6.56348V10.7701C6.33345 10.6707 6.1403 10.5189 6.00553 10.3314C5.87077 10.1439 5.79969 9.92826 5.80021 9.70839C5.79472 9.40209 5.93184 9.10642 6.18152 8.88617C6.4312 8.66592 6.77309 8.53904 7.13228 8.53333C7.49173 8.53888 7.83396 8.66564 8.08404 8.88587C8.33412 9.10611 8.47167 9.40186 8.46658 9.70839C8.46713 9.9284 8.39602 10.1442 8.26116 10.3318C8.12631 10.5194 7.93301 10.6713 7.70282 10.7707V12.2667V12.266Z" fill="#ECF8FC"/><path d="M3.29465 4.74238V6.13333H2.33325V4.74238C2.34666 3.80845 2.63097 2.89861 3.15168 2.12375C3.67239 1.3489 4.40703 0.742472 5.26584 0.378473C5.99052 0.0720971 6.77918 -0.0512279 7.56261 0.0193187C8.34604 0.0898654 9.10017 0.352149 9.75866 0.783077C10.4172 1.21401 10.9599 1.80037 11.339 2.49064C11.7182 3.18091 11.9223 3.95396 11.9333 4.74173V5.93076H10.9729V4.74238C10.9541 3.73493 10.5412 2.77511 9.82325 2.06931C9.10526 1.36351 8.13949 0.968109 7.133