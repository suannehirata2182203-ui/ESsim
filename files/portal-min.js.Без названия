        },
        updateButtonsCookiePage: function () {
            let preferencesConsent= (typeof Cookiebot !== 'undefined' )? Cookiebot.consent.preferences : false;
            let statisticsConsent = (typeof Cookiebot !== 'undefined' )? Cookiebot.consent.statistics : false;
            let marketingConsent = (typeof Cookiebot !== 'undefined' )? Cookiebot.consent.marketing : false;

            if(!preferencesConsent && !statisticsConsent && !marketingConsent){
                $('.actions').html('<a type="button" href="javascript:" onclick="Cookiebot.renew();" class="btn btn-primary">CAMBIAR TU CONSENTIMIENTO</a>');
            }else{
                $('.actions').html('<a type="button" href="javascript:" onclick="Cookiebot.withdraw();" class="btn btn-secondary deny">RETIRAR TU CONSENTIMIENTO</a>' +
                    '<a type="button" href="javascript:" onclick="Cookiebot.renew();" class="btn btn-primary">CAMBIAR TU CONSENTIMIENTO</a>');
            }
        }
    };
