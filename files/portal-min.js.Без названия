                            }
                        }else{
                            $(this).find('span').html(`<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none"><path d="M19.5288 17.2702C20.1537 17.8952 20.1537 18.9076 19.5288 19.5325C19.2188 19.845 18.8088 20 18.3989 20C17.9889 20 17.5799 19.8438 17.268 19.5313L11.9994 14.2654L6.73129 19.53C6.41881 19.845 6.00934 20 5.59988 20C5.19041 20 4.78144 19.845 4.46871 19.53C3.84376 18.9051 3.84376 17.8927 4.46871 17.2677L9.7383 11.9981L4.46871 6.73104C3.84376 6.10609 3.84376 5.09366 4.46871 4.46871C5.09366 3.84376 6.10609 3.84376 6.73104 4.46871L11.9994 9.7408L17.269 4.47121C17.8939 3.84626 18.9063 3.84626 19.5313 4.47121C20.1562 5.09616 20.1562 6.10859 19.5313 6.73354L14.2617 12.0031L19.5288 17.2702Z" fill="#ECF8FC"/></svg>`);
                        }
                        break;
                    case 'vwo':
                        if (consentStatus) {
                            window._vwo_code || (function() {var account_id=736116,version=2.1,settings_tolerance=2000,hide_element='body',hide_element_style = 'opacity:0 !important;filter:alpha(opacity=0) !important;background:none !important',
