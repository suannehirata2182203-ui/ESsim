global.init();

$( document ).ready(function() {
    const lists = $('.gaList, .promotion_ga');
    const gaCtas = $('*[data-ga]');
    class ListsTracker {
        #tracked = [];

        constructor(scroll, galists ) {
            this.scroll = scroll;
            this.galists = galists;
        }


        start(){
            let gaLists = this.galists,
            ctrl = this.#pushCtrl,
            toList = this.#pushList,
            pushEvent = this.#pushEvent,
            objConstructor = this.#objConstructor,
            gaTracked = this.#tracked;
                if (gaLists.length > 0){
                    if (this.scroll){
                        ctrl(gaLists, gaTracked, toList, objConstructor)
                        $(document).on('scroll', function(){
                            gaLists.each( function(ind,elem){
                                ctrl(gaLists, gaTracked, toList, objConstructor)
                            })
                        })
                    }else {
                        gaLists.each(function (ind, elem) {
                            toList(elem,objConstructor)
                        })
                    }
                }
                if (gaCtas.length > 0){
                    gaCtas.on('click', function(ev){
                            $('.modal').find('a').off('click');
                            let elem = this;
                        if ($(this).data('toggle') === 'modal'){
                            let modal = $('.modal'+$(elem).attr('data-target') );
                            modal.find('a').on('click', function(ev){
                                pushEvent(elem, objConstructor)
                            })
                        }else{
                            pushEvent(elem, objConstructor)
                        }

                    })
                }
            };
            #objConstructor(products, event){
                let items = [];
                products.each(function(i, e){
                    let objToGa = {
                        item_name: $(e).attr('data-name') || $(e).attr('data-resume-title'), // Name or ID is required.
                        item_id: $(e).attr('data-id') || $(e).attr('data-package'),
                        price: Number(parseFloat($(e).attr('data-price')).toFixed(2)),
                        item_brand: $(e).attr('data-brand'),
                        item_category: $(e).attr('data-category'),
                        quantity: ($(e).attr('data-units') === undefined)? 1 :parseInt($(e).attr('data-units'))
                    }
                    if(event === 'view_promotion'){
                        objToGa.promotion_id = $(e).attr('data-promoId');
                        objToGa.promotion_name = $(e).attr('data-promoName');
                    }else{
                        objToGa.item_list_name = $(e).attr('data-listName');
                        objToGa.item_list_id = $(e).attr('data-itemListId');
                    }
                    items.push(
                        objToGa
                    )
                })
                return items;
        }
        #pushList(elem, objConstructor){
            let products = $(elem).hasClass('promotion_ga')
                ? $(elem)
                : $(elem).find('.product_ga'),
                event = $(elem).hasClass('promotion_ga')
                    ? 'view_promotion'
                    : 'view_item_list',
                items = objConstructor(products, event);
            dataLayer.push({
                event: event,
                ecommerce: {
                    items: items
                }
            });
        }

        #pushEvent(elem, objConstructor){
            let product;

            switch (true){
                case $(elem).closest('.product_ga').length > 0 :
                    product = $(elem).closest('.product_ga')
                    break
                case $(elem).closest('.card-body').find('.product_ga').length > 0 :
                    product = $(elem).closest('.card-body').find('.product_ga')
                    break
                default:
                    if (DEV_LOGS){
                        console.warn('Cant find product to GA')
                    }
            }
        let item = objConstructor(product, 'view_promotion');
            let eventsToGa = [];

            switch ($(elem).data('ga')){
                case 'promotion':
                    eventsToGa = ['select_promotion']
                    break
                case 'detail':
                    eventsToGa = ['view_item', 'select_item']
                    break
                default:
                    if (DEV_LOGS){
                        console.warn('unexpected event to GA in %c '+product+'', 'color:#e60000')
                    }
            }

            $.each(eventsToGa, function(){
            dataLayer.push({
                event: this,
                ecommerce: {
                    items: item
                }
            });
            })
        }
        #pushCtrl(gaLists,gaTracked,toList, objConstructor){
            gaLists.each(function (ind, elem) {
                const elementPosition = $(elem).offset().top;
                const elementBottom = elementPosition + $(elem).outerHeight();
                const windowTop = $(window).scrollTop();
                const windowBottom = windowTop + $(window).height();
                //console.log('--->',$(elem).css('display'))
                if ((elementBottom > windowTop && elementPosition < windowBottom && windowBottom > elementBottom)&&($.inArray($(elem).attr('id'),gaTracked) === -1 && $(elem).css('display') !== 'none')){
                    gaTracked.push($(elem).attr('id'));
                   // console.log(gaTracked)
                    toList(elem, objConstructor)
                }
            })
        }
    }

    //console.log(lists)
    if ((lists.length > 0 || gaCtas.length > 0)){
        const listWatcher = new ListsTracker(true, lists).start();
    }

    let Analytics = function() {
        let self = this;
        function init(){
            addEventListeners();
        }
        function addEventListeners() {
            $(document).on("click", "#modalC2C #modal_c2c_midigi", function (){
                dataLayer.push({'event': 'GAEvent','eventAction': 'c2c_modal_btn_midigi',});
            });
            $(document).on("click", "#modalC2C #modal_c2c_chat", function (){
                dataLayer.push({'event': 'GAEvent','eventAction': 'c2c_modal_btn_chat',});
            });
            $(document).on("click", "#modalC2C #modal_c2c_1200", function (){
                dataLayer.push({'event': 'GAEvent','eventAction': 'c2c_modal_link_1200',});
            });
            $(document).on("click", "#modalC2C #modal_c2c_phone", function (){
                dataLayer.push({'event': 'GAEvent','eventAction': 'c2c_modal_link_phone',});
            });
            $(document).on("shown.bs.modal", "#howtoSport", function (){
                dataLayer.push({'event': 'PantallaPackDeporte'});
            });
        }

        return {
            init: function (){
                init();
            }
        }
    }();


///////////////////////////////////////////////////////

    function cartProductTracker(){
        if (!$('#configurator').length || !$('.money').length){
            return false
        }

        let cart = {fiber:null,land:null,lines:[]},
            cartNew = {fiber:null,land:null,lines:[]};
        const targetNode = document.getElementsByClassName('price-container')[0];

        const observer = new MutationObserver(function(mutations) {
            mutations.forEach(function(mutation) {
                cartNew = {fiber:null,land:null,lines:[]};
                $('.product_ga').each(function(){
                    if (this.dataset.units > 0 || this.dataset.selected === 'true'){
                        toNewCart(this.dataset.package)
                    }
                })
                compareCarts();
            });
        });

        observer.observe(targetNode, { attributes : true, attributeFilter : ['data-price'] });

        function toNewCart(package){
            const elem = $('*[data-package="'+package+'"]');

            switch (elem[0].dataset.category) {
                case "Fibra":
                    cartNew.fiber = package;
                    break;
                case "Movil":
                    cartNew.lines.push({
                        line:package,
                        units:elem[0].dataset.units,
                        item_name: elem[0].dataset.name || elem[0].dataset.resumeTitle, // Name or ID is required.
                        item_id: elem[0].dataset.id || elem[0].dataset.package,
                        price: Number(parseFloat(elem[0].dataset.price).toFixed(2)),
                        item_brand: elem[0].dataset.brand,
                        item_category: elem[0].dataset.category,
                        item_list_name: elem[0].dataset.listName,
                        item_list_id: elem[0].dataset.itemListId,
                        quantity: Number(elem[0].dataset.units)
                    })
                    break;
                case "Fijo":
                    cartNew.land = package;
                    break;
                case "TV":
                    cartNew.tv = package;
                    break;
                default:
                    console.warn('its not valid product category')
            }
        }
        function compareCarts(){
            //Fiber
            if( cartNew.fiber){
                if(cart.fiber !== cartNew.fiber){
                    if(cart.fiber){
                        gaTracking(cart.fiber, 'remove')
                        // console.log('se quita la antigua ', cart.fiber)
                    }
                    // console.log('aade-->', cartNew.fiber)
                    gaTracking(cartNew.fiber, 'add')
                }
            }
            if (!cartNew.fiber && cart.fiber){
                // console.log('en el nuevo no hay pero ejn el viejo si')
                gaTracking(cart.fiber, 'remove')
            }
            //Land line
            if( cartNew.land){
                if(cart.land !== cartNew.land){
                    if(cart.land){
                        gaTracking(cart.land, 'remove')
                        // console.log('se quita la antigua ', cart.land)
                    }
                    // console.log('aade-->', cartNew.land)
                    gaTracking(cartNew.land, 'add')
                }
            }
            if (!cartNew.land && cart.land){
                // console.log('en el nuevo no hay pero en el viejo si')
                gaTracking(cart.land, 'remove')
            }
            //Lines
            let oldPackages = [],
                newPackages = [];

            $(cart.lines).each(function(){
                if (!oldPackages.includes(this.line)){
                    oldPackages.push(this.line)
                }
            })
            $(cartNew.lines).each(function(){
                let line = this.line,
                    units = this.units;
                if (!newPackages.includes(this.line)){
                    newPackages.push(this.line)
                }

                $(cart.lines).each(function(){
                    if (this.line === line){
                        if (this.units > units){
                            // console.log('se quitan ' + (this.units - units ) + ' lineas de la ' + line )
                            gaTracking(line, 'remove', (this.units - units ))
                        }
                        if (this.units < units){
                            // console.log('se aaden' + (this.units - units ) + ' lineas de la ' + line )
                            gaTracking(line, 'add', (units - this.units ))
                        }
                    }
                })
            })

            const linesOnlyOld = $(oldPackages).not(newPackages).get(),
                  linesOnlyNew = $(newPackages).not(oldPackages).get();

            if (linesOnlyOld.length > 0){
                // console.log('estaba en el antiguo y en el nuevo no (quitarlas)', linesOnlyOld)
                $(linesOnlyOld).each(function(){
                    gaTracking(this, 'remove')
                })

            }

            if (linesOnlyNew.length > 0){
                // console.log('esta en el nuevo y en el antiguo no (aadirlas)', linesOnlyNew )
                $(linesOnlyNew).each(function(){
                    const line = $('*[data-package="'+this+'"]') || $('*[data-id="'+this+'"]');
                    const units = Number(line[0].dataset.units);
                    gaTracking(this, 'add', units)
                })
            }

            cart = cartNew;
        }
        function gaTracking(product, action, units = 1){

            let items = [];
            const productSelector = $('*[data-package="'+product+'"]') || $('*[data-id="'+product+'"]');

            if(productSelector.length > 0){
                items.push({
                    item_name: productSelector.attr('data-name') || productSelector.attr('data-resume-title'), // Name or ID is required.
                    item_id: productSelector.attr('data-id') || productSelector.attr('data-package'),
                    price: Number(parseFloat(productSelector.attr('data-price')).toFixed(2)),
                    item_brand: productSelector.attr('data-brand'),
                    item_category: productSelector.attr('data-category'),
                    item_list_name: productSelector.attr('data-listName'),
                    item_list_id: productSelector.attr('data-itemListId'),
                    quantity: Number(units),
                })
            }else{
                if (cart.lines.length > 1){
                    $(cart.lines).each(function(){
                        if (this.line == product){
                            itemsConstructor(this);
                        }
                    })
                }else{
                    itemsConstructor(cart.lines[0])
                }
            }

            function itemsConstructor(item){
                items.push({
                    item_name: item.item_name, // Name or ID is required.
                    item_id: item.item_id,
                    price: item.price,
                    item_brand: item.item_brand,
                    item_category:item.item_category,
                    item_list_name: item.item_list_name,
                    item_list_id: item.item_list_id,
                    quantity: item.quantity
                })
            }

            if(items.length > 0){
                switch (action) {
                    case 'add':
                        dataLayer.push({
                            event: "add_to_cart",
                            ecommerce: {
                                items: items
                            }
                        });
                        break
                    case 'remove':
                        dataLayer.push({
                            event: "remove_from_cart",
                            ecommerce: {
                                items: items
                            }
                        });
                        break
                    default:
                        console.warn('Unexcpected GA action %c'+action+'', 'color:#e60000');
                }
            }

            // console.clear()
            // const datatoGa =[]
            // window.dataLayer.forEach((el,i)=>{
            //     let elem = {}
            //     if(el.ecommerce){
            //         console.log(el)
            //         elem.action = el.event;
            //         elem.item = el.ecommerce.items[0].item_id;
            //         elem.quantity = el.ecommerce.items[0].quantity;
            //         elem.price = el.ecommerce.items[0].price;
            //         datatoGa.push(elem)
            //     }
            // })
            // console.table(datatoGa);
        }
    }
///////////////////////////////////////////

    cartProductTracker()
    Analytics.init();

});


const requiredText = 'Campo requerido',
    invalidFormat = 'Formato no vlido',
    mustAcceptConditions = 'Debe aceptar las condiciones',
    rechargeAmount = 'Importe incorrecto',
    requiredAmount = 'El importe debe ser entre 0,01 y 150',
    dontMatch = 'Los campos no coinciden',
    invoiceFormat = 'Formato de factura incorrecto',
    mustSelectAddress = 'Debe seleccionar una direccin de las sugerencias',
    uploadText = "Debe adjuntar un archivo",
    formsToValidate = document.getElementsByTagName('form'),
    formsDontSubmit = ['click_to_call', 'check_number', 'check_invoice', 'dealers_search', 'sponsorship_inscriptions', 'calculator'],
    formsToSubmit = ['recharge_number', 'referrals', 'subscribe','contact','job_inscriptions', 'calculator_email'];

// https://just-validate.dev/

const validateForm = (formName) => {
    let repeatEmailId, emailId, tlfId, repeatTlfId;
    const setErrors = () => {
        $('form[name='+formName+'] input').each(function(i,el){
            $(el).removeClass('is-invalid')
            if(el.classList.contains('just-validate-error-field')){
                $(el).addClass('is-invalid')
            }
        })
    }
    const syncUpFields = (ogId, repeatId) => {
        $(document).on('keyup', 'form[name='+formName+'] #'+ ogId +', form[name='+formName+'] #'+ repeatId +'', function(){
            if ($('#'+ ogId +', #'+ repeatId +'').hasClass('just-validate-error-field')){
                window['validate-' + formName].revalidateField('#'+ ogId +'').then(setErrors)
                window['validate-' + formName].revalidateField('#'+ repeatId +'').then(setErrors)
            }
        });
    }

    window['validate-' + formName] = new JustValidate('form[name="'+formName+'"]');

    $('form[name='+formName+'] input, form[name='+formName+'] textarea, form[name='+formName+'] select').each(function(index,el){
        const validateType = $(el).data('val');
        const elemId = $(el).attr('id');

        switch (validateType) {
            case 'name':
                window['validate-' + formName].addField('#'+ elemId+'', [
                {
                    rule: 'required',
                    errorMessage: requiredText,
                },
                {
                    rule:'minLength',
                    value:3,
                    errorMessage: 'El nombre debe contener mas de 3 caracteres',
                    validateBeforeSubmitting: true,
                },
                {
                    rule: 'customRegexp',
                    value: /^[a-zA-Z ]+$/gm,
                    errorMessage: invalidFormat
                }
            ])
                break
            case 'tlf':
                window['validate-' + formName].addField('#'+ elemId+'',[
                    {
                        rule: 'required',
                        errorMessage: requiredText,
                    },
                    {
                        rule: 'number',
                        errorMessage: invalidFormat,
                    },
                    {
                        rule:'minLength',
                        value:9,
                        errorMessage: invalidFormat,
                    },
                    {
                        rule:'maxLength',
                        value:9,
                        errorMessage: invalidFormat,
                    },
                    {
                        rule: 'customRegexp',
                        value: /[9678]{1}[0-9]{8}$/gm,
                        errorMessage: invalidFormat,
                    }
                ])
                break

            case 'tlf-compare1':
                repeatTlfId = $('form[name='+formName+'] input[data-val=tlf-compare2]').attr('id');
                window['validate-' + formName].addField('#'+ elemId+'',[
                    {
                        rule: 'required',
                        errorMessage: requiredText,
                    },
                    {
                        rule: 'number',
                        errorMessage: invalidFormat,
                    },
                    {
                        rule:'minLength',
                        value:9,
                        errorMessage: invalidFormat,
                    },
                    {
                        rule:'maxLength',
                        value:9,
                        errorMessage: invalidFormat,
                    },
                    {
                        rule: 'customRegexp',
                        value: /[9678]{1}[0-9]{8}$/gm,
                        errorMessage: invalidFormat,
                    },
                    {
                        validator: (value,fields) => {
                            if (
                                fields['#'+ repeatTlfId + ''] &&
                                fields['#'+ repeatTlfId + ''].elem
                            ){
                                const repeatTlfValue = fields['#'+ repeatTlfId + ''].elem.value;
                                return value === repeatTlfValue;
                            }
                        },
                        errorMessage: dontMatch,
                    }
                ])
                break

            case 'tlf-compare2':
                tlfId = $('form[name='+formName+'] input[data-val=tlf-compare1]').attr('id');
                window['validate-' + formName].addField('#'+ elemId+'',[
                    {
                        rule: 'required',
                        errorMessage: requiredText,
                    },
                    {
                        rule: 'number',
                        errorMessage: invalidFormat,
                    },
                    {
                        rule:'minLength',
                        value:9,
                        errorMessage: invalidFormat,
                    },
                    {
                        rule:'maxLength',
                        value:9,
                        errorMessage: invalidFormat,
                    },
                    {
                        rule: 'customRegexp',
                        value: /[9678]{1}[0-9]{8}$/gm,
                        errorMessage: invalidFormat,
                    },
                    {
                        validator: (value,fields) => {
                            if (
                                fields['#'+ tlfId + ''] &&
                                fields['#'+ tlfId + ''].elem
                            ){
                                const tlfValue = fields['#'+ tlfId + ''].elem.value;
                                return value === tlfValue;
                            }
                        },
                        errorMessage: dontMatch,
                    }
                ])
                break

            case 'email':
                window['validate-' + formName].addField('#'+ elemId+'',[
                    {
                        rule: 'required',
                        errorMessage: requiredText,
                    },
                    {
                        rule:'email',
                        errorMessage: invalidFormat,
                    }
                ])
                break
            case 'email-no-req':
                window['validate-' + formName].addField('#'+ elemId+'',[
                    {
                        rule:'email',
                        errorMessage: invalidFormat,
                    }
                ])
                break
            case 'email-compare1':
                repeatEmailId = $('form[name='+formName+'] input[data-val=email-compare2]').attr('id');
                window['validate-' + formName].addField('#'+ elemId+'',[
                    {
                        rule: 'required',
                        errorMessage: requiredText,
                    },
                    {
                        rule:'email',
                        errorMessage: invalidFormat,
                    },
                    {
                        validator: (value,fields) => {
                            if (
                                fields['#'+ repeatEmailId + ''] &&
                                fields['#'+ repeatEmailId + ''].elem
                            ){
                                const repeatEmailValue = fields['#'+ repeatEmailId + ''].elem.value;
                                return value === repeatEmailValue;
                            }
                        },
                        errorMessage: dontMatch,
                    }
                ])
                break

            case 'email-compare2':
                emailId = $('form[name='+formName+'] input[data-val=email-compare1]').attr('id');
                window['validate-' + formName].addField('#'+ elemId+'',[
                    {
                        rule: 'required',
                        errorMessage: requiredText,
                    },
                    {
                        rule:'email',
                        errorMessage: invalidFormat,
                    },
                    {
                        validator: (value,fields) => {
                            if (
                                fields['#'+ emailId + ''] &&
                                fields['#'+ emailId + ''].elem
                            ){
                                const emailValue = fields['#'+ emailId + ''].elem.value;
                                return value === emailValue;
                            }
                        },
                        errorMessage: dontMatch,
                    }
                ])
                break

            case 'txt':
                window['validate-' + formName].addField('#'+ elemId+'',[
                    {
                        rule: 'required',
                        errorMessage: requiredText,
                    }
                ])
                break
            case 'number':
                window['validate-' + formName].addField('#'+ elemId+'',[
                    {
                        rule: 'required',
                        errorMessage: requiredText,
                    },
                    {
                        rule: 'number',
                        errorMessage: invalidFormat,
                    }
                ])
                break
            case 'amount':
                window['validate-' + formName].addField('#'+ elemId+'',[
                    {
                        rule: 'required',
                        errorMessage: requiredText,
                    },
                    {
                        rule: 'number',
                        errorMessage: invalidFormat,
                    },
                    {
                        rule: 'minNumber',
                        value: 0.01,
                        errorMessage: requiredAmount,
                    },
                    {
                        rule: 'maxNumber',
                        value: 150,
                        errorMessage: requiredAmount,
                    }
                ])
                break

            case 'checked':
                window['validate-' + formName].addField('#'+ elemId+'',[
                    {
                        rule: 'required',
                        errorMessage: mustAcceptConditions,
                    }
                ])
                break

            case 'recharge-amount':
                window['validate-' + formName].addField('#'+ elemId+'',[
                    {
                        rule: 'required',
                        errorMessage: requiredText,
                    },
                    {
                        validator: (value, context) => {
                            let vl = parseInt(value);
                            if((vl % 5 !== 0 && vl !== 7 && vl !== 3 && vl !== 12) || vl > 50 || vl < 3){
                               return false
                            }else{
                               return true
                            }
                        },
                        errorMessage: rechargeAmount
                    }
                ])
                break

            case 'client':
                window['validate-' + formName].addField('#'+ elemId+'', [
                    {
                        rule: 'required',
                        errorMessage: requiredText,
                    },

                    {
                        rule: 'customRegexp',
                        value: /^(\+|-)?\d+/gm,
                        errorMessage: invalidFormat
                    }
                ])
                break

            case 'invoice':
                window['validate-' + formName].addField('#'+ elemId+'', [
                    {
                        rule: 'required',
                        errorMessage: requiredText,
                    },
                    {
                        rule: 'customRegexp',
                        value: /^([a-zA-Z]{4,5})[\d]{9,}/gm,
                        errorMessage: invoiceFormat
                    }
                ])
                break

            case 'dnie':
                window['validate-' + formName].addField('#'+ elemId+'', [
                    {
                        rule: 'required',
                        errorMessage: requiredText,
                    },
                    {
                        validator:(value)=>{
                            let validChars = 'TRWAGMYFPDXBNJZSQVHLCKET';
                            let nifRexp = /^[0-9]{8}[TRWAGMYFPDXBNJZSQVHLCKET]{1}$/i;
                            let nieRexp = /^[XYZ]{1}[0-9]{7}[TRWAGMYFPDXBNJZSQVHLCKET]{1}$/i;
                            let str = value.toString().toUpperCase();
                            if (!nifRexp.test(str) && !nieRexp.test(str)) return false;
                            let nie = str
                                .replace(/^[X]/, '0')
                                .replace(/^[Y]/, '1')
                                .replace(/^[Z]/, '2');
                            let letter = str.substr(-1);
                            let charIndex = parseInt(nie.substr(0, 8)) % 23;
                            if (validChars.charAt(charIndex) === letter) return true;

                            return false;
                        },
                        errorMessage:invalidFormat
                    }
                ])
                break
            case 'cp':
                window['validate-' + formName].addField('#'+ elemId+'', [
                    {
                        rule: 'required',
                        errorMessage: requiredText,
                    },
                    {
                        rule: 'customRegexp',
                        value: /^(?:0?[1-9]|[1-4]\d|5[0-2])\d{3}$/gm,
                        errorMessage: invalidFormat
                    }
                ])
                break
            case 'fileUpload1':
                window['validate-' + formName].addField('#'+ elemId+'', [
                    {
                        rule: 'minFilesCount',
                        value: 1,
                        errorMessage: uploadText
                    }
                ])
                break
            case 'select':
                window['validate-' + formName].addField('#'+ elemId+'',[
                    {
                        rule: 'required',
                        errorMessage: requiredText,
                    },
                    {   validator: (value) => {

                            if (value.trim() === '') return false;

                            return true;

                        }, errorMessage:requiredText
                    }
                ])
                break
            case 'date':
                window['validate-' + formName].addField('#'+ elemId+'',[
                    {
                        rule: 'required',
                        errorMessage: requiredText,
                    },
                    {
                        validator: (value) => {
                            const selectedDate = new Date(value);
                            const today = new Date();

                            selectedDate.setHours(0, 0, 0, 0);
                            today.setHours(0, 0, 0, 0);

                            if(selectedDate.getTime() < today.getTime()){
                                return false;
                            }
                            return true;

                        }, errorMessage: "La fecha debe ser mayor o igual a hoy."
                    }
                ])
                break
            case 'numberMax':
                const minVal = Number(el.getAttribute('min'));
                const maxVal = Number(el.getAttribute('max'));
                window['validate-' + formName].addField('#'+ elemId+'',[
                    {
                        rule: 'required',
                        errorMessage: requiredText,
                    },
                    {
                        rule: 'number',
                        errorMessage: invalidFormat,
                    },
                    {
                        rule: 'minNumber',
                        value: minVal,
                        errorMessage: `El importe debe ser entre ${minVal} y ${maxVal}`,
                    },
                    {
                        rule: 'maxNumber',
                        value: maxVal,
                        errorMessage: `El importe debe ser entre ${minVal} y ${maxVal}`,
                    }
                ])
                break
            default:
                if (DEV_LOGS){
                    console.warn(elemId+' dont have validation')
                }
        }
    })

    window['validate-' + formName].onValidate(setErrors)
    if(formsToSubmit.includes(window['validate-' + formName].form.name) && !formsDontSubmit.includes(window['validate-' + formName].form.name)){
        window['validate-' + formName].onSuccess(()=>$('form[name='+formName+']').submit())
    }


    syncUpFields(emailId,repeatEmailId)
    syncUpFields(tlfId,repeatTlfId)

    if(formName === 'recharge_number'){
        $('input[value="5"]').trigger('click');
        const observer = new MutationObserver(function(mutationList, observer) {
            mutationList.forEach(function(mutation) {
                if (mutation.attributeName === "style" && $('form[name="recharge_number"]').css('display')==='block'){
                    $('input[value="5"]').trigger('click');
                }
            });
        });

        const elementNode = document.querySelector('form[name="recharge_number"]');

        observer.observe(elementNode, {
            attributes: true,
            childList: false,
            subtree: false }
        );
    }

}
$(formsToValidate).each(function(index,el){
    if(formsToSubmit.includes(el.name) || formsDontSubmit.includes(el.name)){
        validateForm(el.name)
    }
})
